
@model IEnumerable<VSG.Models.DatDichvu>

@{
    ViewBag.Title = "Datdichvu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    table {
        margin: 0 auto;
        width: 800px;
    }
</style>


<body>
    <table cellpadding="15" border="1">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SOid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoTen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SDT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgayDat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GhiChu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SOid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HoTen)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SDT)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Title)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NgayDat)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GhiChu)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>

                    @Html.ActionLink("Delete", "Delete", new { id = item.SOid })
                </td>

            </tr>
        }

    </table>
</body>